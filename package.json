{
  "name": "webapp-v2",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@0x/contract-addresses": "^6.3.0",
    "@0x/utils": "^6.4.4",
    "@craco/craco": "^6.1.2",
    "@gnosis.pm/safe-apps-web3-react": "^0.6.5",
    "@headlessui/react": "^1.3.0",
    "@popperjs/core": "^2.9.3",
    "@reduxjs/toolkit": "^1.6.2",
    "@tailwindcss/forms": "^0.3.3",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.1.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "@web3-react/walletlink-connector": "^6.2.3",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "dayjs": "^1.10.5",
    "ethers": "^5.5.1",
    "json-bigint": "^1.0.0",
    "lightweight-charts": "^3.5.0",
    "lodash": "^4.17.21",
    "numbro": "^2.3.5",
    "numeral": "^2.0.6",
    "react": "^17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.17.6",
    "react-popper": "^2.2.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "rxjs": "6.6.7",
    "source-map-explorer": "^2.5.2",
    "use-async-effect": "^2.2.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@typechain/ethers-v5": "^8.0.2",
    "@types/json-bigint": "^1.0.0",
    "@types/numeral": "^2.0.1",
    "@types/react-table": "^7.7.2",
    "autoprefixer": "^9",
    "cypress": "^9.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "is-ci": "^3.0.1",
    "is-ci-cli": "^2.2.0",
    "lint-staged": "^11.0.0",
    "msw": "^0.35.0",
    "postcss": "^7",
    "prettier": "^2.3.0",
    "react-redux": "^7.2.6",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.2.4",
    "typechain": "^6.0.2",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "TAILWIND_MODE=watch craco start",
    "start:cli": "BROWSER=none craco start",
    "build": "craco build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:jest": "craco test",
    "test:coverage": "CI=1 yarn test:jest --coverage",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test start:cli http://localhost:3000 cy:open",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "start-server-and-test serve http://localhost:3000 cy:run",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "serve": "serve --no-clipboard --single --listen 3000 build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "compile-abis": "typechain --target ethers-v5 --out-dir src/services/web3/abis/types './src/services/web3/abis/*.json'",
    "postinstall": "yarn compile-abis",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/test/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
