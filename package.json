{
  "name": "webapp-v2",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@0x/contract-addresses": "^6.13.0",
    "@gnosis.pm/safe-apps-web3-react": "^0.6.5",
    "@headlessui/react": "^1.3.0",
    "@popperjs/core": "^2.11.5",
    "@reduxjs/toolkit": "^1.8.1",
    "@tailwindcss/forms": "^0.5.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/torus-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "@web3-react/walletlink-connector": "^6.2.3",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.1",
    "chart.js": "^3.7.0",
    "cypress": "^8.0.0",
    "dayjs": "^1.11.2",
    "ethers": "^5.6.5",
    "json-bigint": "^1.0.0",
    "jszip": "^3.9.1",
    "lightweight-charts": "^3.5.0",
    "lodash": "^4.17.21",
    "numbro": "^2.3.5",
    "numeral": "^2.0.6",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-chartjs-2": "^4.0.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.1.0",
    "react-intl": "^5.25.1",
    "react-popper": "^2.2.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-table": "^7.7.0",
    "rxjs": "6.6.7",
    "source-map-explorer": "^2.5.2",
    "swiper": "^8.1.4",
    "use-async-effect": "^2.2.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/node-logger": "6.3.12",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.3.12",
    "@typechain/ethers-v5": "^8.0.2",
    "@types/json-bigint": "^1.0.0",
    "@types/numeral": "^2.0.1",
    "@types/react-table": "^7.7.2",
    "autoprefixer": "^10.4.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwind": "^2.2.12",
    "react-redux": "^8.0.1",
    "storybook-addon-designs": "^6.2.0",
    "tailwindcss": "^3.0.24",
    "typechain": "^6.0.2",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "compile-abis": "typechain --target ethers-v5 --out-dir src/services/web3/abis/types './src/services/web3/abis/**/*.json'",
    "postinstall": "yarn compile-abis",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "react-error-overlay": "6.0.9"
  }
}
